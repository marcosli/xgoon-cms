{
    "name": "zendframework/skeleton-application",
    "description": "Skeleton Application for ZF2",
    "license": "BSD-3-Clause",
    "keywords": [
        "zf2", "doctrine2", "orm", "zfc", "oauth", "socialauth", "hybridauth"
    ],
    "homepage": "http://framework.zend.com/",
    "require": {
        "php": ">=5.3.3",
        "zendframework/zendframework": "2.2.*",
        "zendframework/zend-developer-tools": "dev-master",
        "doctrine/doctrine-orm-module": "0.7.*",
        "zf-commons/zfc-user": "0.1.*",
        "zf-commons/zfc-user-doctrine-orm": "0.*",
        "bjyoungblood/bjy-authorize": "dev-master",
        "manuakasam/sam-user": "dev-master",
        "zf-commons/zfc-admin": "dev-master",
        "org_heigl/hybridauth": "dev-master",
        "hybridauth/hybridauth": "dev-master",
        "socalnick/scn-social-auth": "1.*",
        "socalnick/scn-social-auth-doctrine-orm": "dev-master",
        "bjyoungblood/bjy-profiler": "dev-master",
        "darkmatus/roleuserbridge": "dev-master"
    },
    "suggest": {
        "bjyoungblood/bjy-authorize": "Access control to protect ZfcAdmin against unauthorized users"
    },
    "autoload": {
        "psr-0": {
            "ZfcAdmin": "src/"
        },
        "psr-0": {
            "RoleUserBridge": "src/"
        },                
        "classmap": [
            "./"
        ]
    }

}
